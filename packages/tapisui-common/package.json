  "description": "Common React components for TapisUI",
  "scripts": {
    "test": "vitest run",
    "clean": "rm -rf dist",
    "build": "npm run clean && npx tsc --build ./tsconfig.json && npm run copy-files",
    "copy-files": "cpx \"src/{ui/**/*.scss,styles/**/*,fonts/**/*,**/*.scss,index.css,**/*.css,**/*.module.css}\" dist",
