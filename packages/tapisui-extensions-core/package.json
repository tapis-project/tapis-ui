{
  "name": "@tapis/tapisui-extensions-core",
  "version": "0.1.10",
  "description": "Typescript library that enables users to create extensions for TapisUI",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "bundle": "node --loader ts-node/esm ./src/bundle.ts",
    "build": "npx tsc",
    "push": "npx tsc && npm publish --access public",
    "watch": "npx tsc --build --watch --preserveWatchOutput"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tapis-project/tapisui-extensions-core.git"
  },
  "keywords": [
    "typescript",
    "tapisui",
    "UT",
    "TACC",
    "CIC"
  ],
  "author": "Nathan Freeman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tapis-project/tapisui-extensions-core/issues"
  },
  "homepage": "https://github.com/tapis-project/tapisui-extensions-core#readme",
  "devDependencies": {
    "@tapis/tapisui-extension-devtools": "file:../tapisui-extension-devtools",
    "@types/node": "^16.0.0",
    "react": "^18.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@tapis/tapis-typescript": "^0.0.56",
    "simple-git": "^3.24.0"
  }
}
