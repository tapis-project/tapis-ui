    "@tapis/tapisui-hooks": "file:./packages/tapisui-hooks",
    "@uiw/codemirror-theme-vscode": "^4.22.2",
    "@uiw/react-codemirror": "^4.19.7",
    "@uiw/react-textarea-code-editor": "^2.0.6",
    "@vitejs/plugin-react": "^4.3.0",
    "@xyflow/react": "^12.0.4",
    "base-64": "^1.0.0",
    "bootstrap": "^4.6.0",
    "formik": "^2.2.9",
    "gantt-task-react": "^0.3.9",
    "js-cookie": "^3.0.0",
    "jwt-decode": "^3.1.2",
