  "scripts": {
    "start": "vite --config ./vite/vite.config.mts --clearScreen=false",
    "start-dev": "vite --host --config ./vite/vite.dev.config.mts",
    "init-project": "[ ! -f .env ] && cp .env.template .env || true && pnpm install && pnpm -r build && pnpm start",
    "dev": "pnpm -r --parallel watch & pnpm run start & (sleep 7 && pnpm run prettier-loop)",
    "build": "tsc && NODE_OPTIONS=--max-old-space-size=9000 vite build --config ./vite/vite.config.mts",
    "build-dev": "tsc && NODE_OPTIONS=--max-old-space-size=9000 vite build --config ./vite/vite.dev.config.mts",
    "preview": "vite preview --config ./vite/vite.config.mts",
    "preview-dev": "vite preview --config ./vite/vite.dev.config.mts",
    "docker": "docker build -t uidev -f Dockerfile.nix .; docker run -p 3000:3000 uidev",
    "docker-prod": "docker build -t uiprod -f Dockerfile.nix .; docker run -p 3000:80 uiprod",
    "test": "chmod +x ./scripts/test-all.sh && ./scripts/test-all.sh",
    "lint": "eslint --max-warnings=0 . && prettier --single-quote --check src",
    "prettier": "prettier --single-quote --write -l src packages",
    "prettier-loop": "while true; do prettier --loglevel=silent --single-quote -l src packages || (echo -e '\n#############################################\nFound something to pretty - checked every 60s'; eslint --max-warnings=0 .; echo 'eslint ran - now running prettier'; prettier --loglevel=log --single-quote --write -l src packages; echo -e '#############################################\n'); sleep 60; done",
    "gh-pages": "pnpm build && npx gh-pages -d dist",
    "taskgen": "docker build -f packages/tapisui-extensions-core/Dockerfile -t tapis/tapisui-task-generator:latest . && docker run --mount type=bind,source=$(pwd),target=/tapisui -it tapis/tapisui-task-generator:latest"
  },
  "eslintConfig": {
    "extends": []
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
